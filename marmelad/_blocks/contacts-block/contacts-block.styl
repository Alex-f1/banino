.contacts-block
  position: relative
  margin-top: -105px
  padding-bottom: 60px

  @media (max-width: 959px)
    margin-top: -65px

  &__map
    width: 100%
    height: 740px

    @media (max-width: 959px)
      height: 450px

  .ymaps-2-1-79-ground-pane
    filter: url('data:image/svg+xml;utf8,<svg xmlns=\"http://www.w3.org/2000/svg\"><filter id=\"grayscale\"><feColorMatrix type=\"matrix\" values=\"0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0 0 0 1 0\"/></filter></svg>#grayscale')
    filter: grayscale(100%)

  &__wrapper
    width: 1170px
    position: absolute
    top: 0
    padding-top: 125px
    display: flex
    justify-content: space-between

    @media (max-width: 1279px)
      width: 900px

    @media (max-width: 959px)
      flex-direction: column
      align-items: center
      padding-top: 60px
      height: 740px
      width: 100%
      margin-left: -15px

  &__info
    color: #fff
    padding: 50px 75px 50px 85px
    background-color: #A49777
    box-shadow: 0px 4px 34px rgba(0, 0, 0, 0.17)
    border-radius: 25px
    max-width: 455px

    @media (max-width: 959px)
      margin-top: auto

    @media (max-width: 767px)
      padding: 30px

  &__title
    font-size: 28px
    color: #FFF
    font-weight: 700
    line-height: 1.2
    margin-bottom: 30px

    @media (max-width: 767px)
      font-size: 22px

  &__item
    font-size: 18px
    color: #FFF
    font-weight: 500
    line-height: 1.1
    display: flex
    align-items: center
    margin-bottom: 20px

    @media (max-width: 767px)
      font-size: 16px

    .svg-icon
      margin-right: 20px

    a
      font-size: 18px
      color: #FFF
      font-weight: 500
      line-height: 1.1
      text-decoration: none

      @media (max-width: 767px)
        font-size: 16px

  &__socials
    display: flex
    align-items: center
    justify-content: center
    margin-top: 50px

  &__social-link
    flex: 0 0 25px
    width: 25px
    max-width: 25px
    height: 25px
    background-repeat: no-repeat
    background-size: contain
    background-position: 0 0
    margin-right: 15px
    transition: background 0.3s, ease 0.3s

    @media (max-width: 767px)
      flex: 0 0 25px
      width: 25px
      max-width: 25px
      height: 25px
      margin-right: 15px

