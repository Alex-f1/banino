  &__call
    width: 230px
    display: flex
    flex-direction: column
    align-items: center
    text-align: center

    @media (max-width: 959px)
      order: -1

    a
      margin-bottom: 20px

    span
      font-size: 18px
      color: #3E3E3E
      line-height: 1.1
      font-weight: 500

      br
        @media (max-width: 959px)
          display: none


  &__bottom
    margin-top: 45px

    @media (max-width: 959px)
      padding-top: 300px

  &__content
    max-width: 800px
    width: 100%
    display: flex
    margin-right: auto
    margin-left: auto

    @media (max-width: 767px)
      display: block

  &__data-info
    width: 50%
    margin-right: 70px

    @media (max-width: 767px)
      width: 100%
      margin-right: 0
      margin-bottom: 50px

    &:last-child
      margin-right: 0
      margin-bottom: 0

    strong
      font-size: 22px
      color: #373737
      font-weight: 700
      line-height: 1.1
      display: block
      margin-bottom: 20px

    p
      font-size: 20px
      color: #636363
      font-weight: 500
      line-height: 1.4
